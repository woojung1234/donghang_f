# React App Environment Variables Example
# Copy this file to .env and fill in your actual values

# 백엔드 API 서버 URL
REACT_APP_API_URL=http://localhost:9090

# AI 서비스 URL  
REACT_APP_AI_URL=http://localhost:8000

# 애플리케이션 환경
REACT_APP_ENV=development

# 디버그 모드
REACT_APP_DEBUG=true

# 공공 데이터 포털 API 키
REACT_APP_PUBLIC_DATA_API_KEY=your_public_data_api_key_here

# 인천 API 키
REACT_APP_INCHEON_API_KEY=your_incheon_api_key_here
